{
  "name": "unused-unloved",
  "version": "1.0.3",
  "description": "Find all the unused files ðŸ—‚ by checking all unused imports in your React project (no need for a webpack plugin). List out all those files in a nice JSON file to do what you like with!",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib /**/*"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "tslint -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "preferGlobal": true,
  "bin": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cwatson88/unused-unloved.git"
  },
  "keywords": [
    "unused",
    "import",
    "react",
    "imports",
    "files"
  ],
  "author": "cwatson88",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/cwatson88/unused-unloved/issues"
  },
  "homepage": "https://github.com/cwatson88/unused-unloved#readme",
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/uuid": "^3.4.4",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "parcel-plugin-shebang": "^1.2.3",
    "prettier": "^1.15.3",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@types/inquirer": "0.0.43",
    "@types/node": "^10.12.18",
    "chalk": "^2.4.1",
    "clui": "^0.3.6",
    "commander": "^2.19.0",
    "inquirer": "^6.2.1",
    "minimist": "^1.2.0",
    "uuid": "^3.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx)"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run prepublishOnly",
      "pre-push": "npm run format && npm run prepublishOnly"
    }
  }
}
